# Switch 0 (SW0) to input port A
set_property PACKAGE_PIN J15 [get_ports A]
set_property IOSTANDARD LVCMOS33 [get_ports A]

# Switch 1 (SW1) to input port B
set_property PACKAGE_PIN L16 [get_ports B]
set_property IOSTANDARD LVCMOS33 [get_ports B]

# LED 0 (LD0) to output port X
set_property PACKAGE_PIN H17 [get_ports X]
set_property IOSTANDARD LVCMOS33 [get_ports X]